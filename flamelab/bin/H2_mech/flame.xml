<?xml version="1.0" encoding="ISO-8859-1"?>
<flamecode>
  <reactor model="premix">
    <length unit="cm"> 2 </length>
    <ncells>70</ncells>
    <aspect_ratio>0.2</aspect_ratio>
  </reactor>
  <op_condition>
    <temperature condition="adiabatic"> 1073.15 </temperature>
    <pressure unit="atm" mode="constant">0.0329</pressure>
    <diffusion>on</diffusion>
  </op_condition>
  <inlet>
     <fuel>
       <velocity unit="m/s">1.0</velocity>
       <temperature unit="K">900.7</temperature>
       <!--flowrate unit="cgs">4.63e-3</flowrate-->
       <molefrac>
        <species name="H2">0.28</species>
        <species name="O2">0.09</species>
        <species name="AR">*</species>
       </molefrac>
     </fuel>
     <!--oxidizer>
       <velocity unit="m/s">0.02</velocity>
       <temperature unit="K">800</temperature>
       <massfrac>
        <species name="O2">0.14</species>
        <species name="N2">*</species>
       </massfrac>
     </oxidizer-->
  </inlet>
  <solver mode="transient">
   <aTol>1e-07</aTol>
   <rTol>1.e-07</rTol>
   <iniStep>1.e-06</iniStep>
   <maxStep>0</maxStep>
   <!--minStep>0</minStep-->
   <tMax>0</tMax>
   <output>
     <interval from="0" to="1">1e-03</interval>
     <interval from="1" to="10">5</interval>
     <interval from="10" to="*">10000</interval>
   </output>
  </solver>
  <initialize>
    <massfrac>
      <species name="H2">0.1</species>
      <species name="O2">0.3</species>
      <species name="N2">*</species>
    </massfrac>
    <Tprofile unit_L="cm" unit_T="K">
      <position x="0.0">373.7</position>
      <position x="0.125">484.5</position>
      <position x="0.25">583.7</position>
      <position x="0.375">672.2</position>
      <position x="0.5">753.5</position>
      <position x="0.75">901.4</position>
      <position x="1.0">1027.0</position>
      <position x="1.25">1120.0</position>
      <position x="1.5">1184.0</position>
      <position x="2.0">1260.0</position>
      <position x="3.0">1348.0</position>
      <position x="6.0">1475</position>
      <position x="10.0">1524.0</position>
    </Tprofile>
  </initialize>
  <monitor mode="on" species="MOLE">
  <species name="H2"></species>
  <species name="O2"></species>
  <species name="H2O"></species>
  </monitor>
</flamecode>