<?xml version="1.0" encoding="ISO-8859-1"?>
<flamecode>
  <reactor model="premix">
    <length unit="cm"> 10 </length>
    <ncells>30</ncells>
    <aspect_ratio from="0" to="10" ncells="30">1.15</aspect_ratio>
  </reactor>
  <op_condition>
    <temperature condition="userdefined"> 1073.15 </temperature>
    <pressure unit="atm" mode="constant">0.08</pressure>
	 <diffusion>on</diffusion>
  </op_condition>
  <inlet>
     <fuel>
       <velocity unit="m/s">0.4101</velocity>
       <temperature unit="K">3.03e2</temperature>
       <!--flowrate unit="cgs">4.63e-3</flowrate-->
       <molefrac>
        <species name="CH4">9.5e-02</species>
        <!--species name="CH3">5.867e-10</species>
        <species name="CH2O">1.99e-11</species>
        <species name="CO2">3.314e-10</species>
        <species name="CO">4.357e-09</species>
        <species name="H2">2.443e-06</species-->
        <species name="O2">1.9e-01</species>
        <species name="N2">*</species>
       </molefrac>
     </fuel>
     <!--oxidizer>
       <velocity unit="m/s">0.02</velocity>
       <temperature unit="K">800</temperature>
       <massfrac>
        <species name="O2">0.14</species>
        <species name="N2">*</species>
       </massfrac>
     </oxidizer-->
  </inlet>
  <solver mode="transient">
   <aTol>1e-06</aTol>
   <rTol>1.e-04</rTol>
   <resTol>1.e-06</resTol>
   <iniStep>0</iniStep>
   <maxStep>0</maxStep>
   <!--minStep>0</minStep-->
   <tMax>0</tMax>
   <output mode="final">
     <interval from="0" to="1.0">1e-05</interval>
     <interval from="1.0" to="10">1</interval>
     <interval from="10" to="*">100</interval>
   </output>
  </solver>
  <initialize>
    <molefrac>
      <species name="CO">0.08</species>
      <species name="HCO">0.00001</species>
      <species name="HO2">0.0001</species>
      <species name="O">0.0001</species>
      <species name="H2O2">0.0001</species>
      <species name="H">0.02</species>
      <species name="H2">0.01</species>
      <species name="OH">0.001</species>
      <species name="CH2">0.0001</species>
      <species name="CH">0.001</species>
      <species name="CH2O">0.0001</species>
      <species name="CH3">*</species>
    </molefrac>
    <Tprofile unit_L="cm" unit_T="K" fit="linear">
      <position x="0.0">3.03e2</position>
      <position x="0.025">3.14e2</position>
      <position x="0.0375">3.37e2</position>
      <position x="0.05">4.00e2</position>
      <position x="0.055">4.52e2</position>
      <position x="0.06">5.30e2</position>
      <position x="0.0656">6.57e2</position>
      <position x="0.075">9.66e2</position>
      <position x="0.0787">1.12e3</position>
      <position x="0.0825">1.27e3</position>
      <position x="0.0881">1.49e3</position>
      <position x="0.09">1.56e3</position>
      <position x="0.0975">1.73e3</position>
      <position x="0.1">1.76e3</position>
      <position x="0.12">1.86e3</position>
      <position x="0.15">1.93e3</position>
      <position x="0.24">2.04e3</position>
      <position x="0.3">2.07e3</position>
      <position x="0.54">2.13e3</position>
      <position x="0.7812">2.16e3</position>
      <position x="1.26">2.19e3</position>
      <position x="2.0">2.21e3</position>
      <position x="3.0">2.23e3</position>
      <position x="4.0">2.23e3</position>
      <position x="5.0">2.23e3</position>
      <position x="6.0">2.23e3</position>
      <position x="7.0">2.23e3</position>
      <position x="8.0">2.23e3</position>
      <position x="9.0">2.23e3</position>
      <position x="10.0">2.23e3</position>
    </Tprofile>
  </initialize>
  <monitor mode="on" species="MOLE">
  <species name="H2"></species>
  <species name="O2"></species>
  <species name="H2O"></species>
  </monitor>
</flamecode>
