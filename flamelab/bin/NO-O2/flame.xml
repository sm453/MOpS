<?xml version="1.0" encoding="ISO-8859-1"?>
<flamecode>
  <reactor model="plug">
    <length unit="cm"> 5 </length>
    <ncells>100</ncells>
    <aspect_ratio>0.2</aspect_ratio>
  </reactor>
  <op_condition>
    <temperature condition="adiabatic"> 1073.15 </temperature>
    <pressure unit="Pa" mode="constant">1e5</pressure>
    <diffusion>on</diffusion>
  </op_condition>
  <inlet>
     <fuel>
       <velocity unit="m/s">0.1</velocity>
       <temperature unit="C">800</temperature>
       <massfrac>
        <species name="NO2">0.1</species>
        <species name="N2">*</species>
       </massfrac>
     </fuel>
     <!--oxidizer>
       <velocity unit="m/s">0.02</velocity>
       <temperature unit="K">800</temperature>
       <massfrac>
        <species name="O2">0.14</species>
        <species name="N2">*</species>
       </massfrac>
     </oxidizer-->
  </inlet>
  <solver mode="steady">
   <aTol>1e-09</aTol>
   <rTol>1.e-06</rTol>
   <iniStep>1.e-06</iniStep>
   <maxStep>0</maxStep>
   <tMax>0</tMax>
   <output>
     <interval from="0" to="10">0.1</interval>
     <interval from="10" to="50">5</interval>
     <interval from="50" to="*">10000</interval>
   </output>
  </solver>
  <initialize>
    <massfrac>
      <species name="NO2">0.1</species>
      <species name="N2">*</species>
    </massfrac>
    <Tprofile unit_L="cm" unit_T="K">
      <position x="0.0">300.16</position>
      <position x="0.2">300.00</position>
      <position x="0.5">300.00</position>
      <position x="1.0">302.27</position>
      <position x="1.2">328.91</position>
      <position x="1.3">373.50</position>
      <position x="1.5">590.72</position>
      <position x="1.7">979.45</position>
      <position x="1.8">1177.31</position>
      <position x="1.9">1320.58</position>
      <position x="2.0">1373.00</position>
      <position x="2.3">979.45</position>
      <position x="2.5">590.72</position>
      <position x="2.6">457.36</position>
      <position x="2.8">328.91</position>
      <position x="3.0">302.27</position>
      <position x="4.0">300.16</position>
    </Tprofile>
  </initialize>
  <monitor mode="on">
  <species name="O2"> </species>
  <species name="NO2"></species>
  </monitor>
</flamecode>