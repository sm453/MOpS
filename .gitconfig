[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
    autocrlf = true
[branch "master"]
    remote = origin
    merge = refs/heads/master
[remote "origin"]
    url = https://github.com/ucam-ceb-como/MOpS.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[alias]
    logf = log --follow
    lg = log --follow --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
