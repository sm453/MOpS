<?xml version="1.0" encoding="ISO-8859-1"?>

<mechanism name="Rob test soot 1" units="CGS">
    
    
    <!-- Define particle components. -->
    
    <component id="pah" type="bulk">
      <description>
        amorphous carbon
      </description>
      <density units="g/cm3">1.8</density>
      <molwt units="g/mol">12</molwt>
    </component> 
    

    <particle id="[soot]" model="spherical" subtree="false">
      <description>
        A soot particle.
      </description>
    </particle>
 
    <inception name="Dimer inception">
        <formula>A4 + A4 to [X]</formula>
        <reactant id="A4" stoich="2" m="3.19e-22" d="7.109e-8"/>
        <particleout id="[X]"/>
        <component id="pah" dx="32"/>
        <A>1.0</A>
    </inception>

    <condensation name="pyrene" defer="false"> 
        <formula>[X] + A4 to [X+1]</formula>        
        <reactant id="A4" stoich="1" m="3.19e-22" d="7.109e-8"/>
        <component id="pah" dx="+16"/>
        <A>1.0</A>
    </condensation>

    <reaction name="sgrowth" defer="true" type="surface">
        <formula>[X] + C2H2 to [X+1]</formula>        
        <reactant id="C2H2" stoich="1"/>
        <product id="H" stoich="1"/>
        <component id="pah" dx="+2"/>
        <A>1.4e43</A>
        <n>-3.36</n>
        <E>26.7</E>
        <particleterm id="s" power="1"/>
    </reaction>

    <reaction name="sox" defer="true" type="surface">
        <formula>[X] + OH to [X-1]</formula>        
        <reactant id="OH" stoich="1"/>
        <component id="pah" dx="-1"/>
        <A>2.074e30</A>
        <n>0.5</n>
        <particleterm id="d" power="2"/>
    </reaction>


    <coagulation>
      <kernel>transition</kernel>
      <A>1.0</A>
    </coagulation>

</mechanism>
