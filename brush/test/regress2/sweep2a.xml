<?xml version="1.0" encoding="ISO-8859-1"?>

<mechanism name="Rob test soot 1" units="CGS">

    <model type="drag" id="Temperature">
        <!-- This value effectively turns off the simulation
             of diffusion in the split particle transport
             code by forcing the diffusion co-efficient to be
             almost 0. -->
        <A>1e100</A>
    </model>
    
    <!-- Define particle components. -->
    
    <component id="pah" type="bulk">
      <description>
        amorphous carbon
      </description>
      <density units="g/cm3">1</density>
      <molwt units="g/mol">6.0221415e26</molwt>
    </component> 
    

    <particle id="[soot]" model="spherical" subtree="false">
      <description>
        A soot particle.
      </description>
    </particle>

    
    <!-- Inceptions -->

    <!--inception name="Dimer inception">
        <formula>A4 + A4 to [X]</formula>
        <reactant id="A4" stoich="2" m="9.981e-23" d="0.32e-8"/>
        <particleout id="[X]"/>
        <component id="pah" dx="32"/>
        <A>1.0</A>
    </inception-->
 
    <coagulation>
        <kernel>additive</kernel>
        <A>0.0</A>
    </coagulation>

    <diffusion name="uniform">
        <particleterm id="d" power="0" />
        <A>1.0</A>
    </diffusion>

</mechanism>
