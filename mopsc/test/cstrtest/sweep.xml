<?xml version="1.0" encoding="ISO-8859-1"?>

<mechanism name="Titania nanoparticle growth model">

    <!-- Define particle types and components. -->
    
    <particle id="[X]">
      <sintering model="default" enable="false">
        <A>1.838e6</A>
        <B>2.766e4</B>
      </sintering>
    </particle>
    
    <component id="titania">
        <density units="g/cm3">2.2</density>
        <molwt units="g/mol">60.084</molwt>
    </component>
   
    <!-- Inceptions -->
    
    <inception name="Dimer inception">
        <formula>TIO2 + TIO2 to [X]</formula>
        <reactant id="TIO2" stoich="2" m="9.981e-23" d="0.32e-7"/>
        <particleout id="[X]"/>
        <component id="titania" dx="2"/>
        <A>0.0</A>
    </inception>

    <!-- Surface condensation reactions (single particle events) -->
    
    <reaction type="condensation" name="TiO2 condensation" defer="true">
        <formula>[X] + TIO2 to [X+1]</formula>        
        <reactant id="TIO2" stoich="1" m="9.981e-23" d="0.32e-7"/>
        <component id="titania" dx="+1"/>
    </reaction>
    
</mechanism>
