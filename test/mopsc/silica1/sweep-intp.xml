<?xml version="1.0" encoding="ISO-8859-1"?>

<mechanism name="Silica nanoparticle inception and growth model" units="CGS">
    <component id="silica" type="bulk">
      <description>Silica</description>
      <density units="g/cm3">2.2</density>
      <molwt units="g/mol">60.084</molwt>
      <coalthresh>1</coalthresh>
    </component> 

    <particle id="[silica]" model="silica" subtree="false">
      <description>A silica particle.</description>
      <fractdim>1.8</fractdim>
      <sintering enable="false" model="viscous_flow">
        <A units="s/m">1.1e-16</A>
        <E units="K">1.2e5</E>
        <Dpmin units="m">4.4e-9</Dpmin>
      </sintering>
    </particle>
    
    <coagulation>
        <A>0.0</A>
        <kernel>transition</kernel>
    </coagulation>
    
    <interparticle type="inter particle" name="SiO2 interparticle reaction" defer="false">
        <formula>[X] to [Y] + H2O</formula>        
        <!--NOTE that H4O4SI must NOT be labelled as a reactant, as this
        leads to the erroneous removal of reactants from the gasphase-->
        <!--reactant id="H4O4SI" stoich="1"/-->
        <product id="H2O" stoich="1"/>
        <component id="silica" dx="0"/>
        <A>1e13</A>
        <n>0.0</n>
        <E units="cal">0</E>
        <particleterm id="asn" power="1"/>        
    </interparticle>

</mechanism>

