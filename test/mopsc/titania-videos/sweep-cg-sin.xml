<?xml version="1.0" encoding="ISO-8859-1"?>
<mechanism name="Titanium Dioxide" units="CGS">

	<!-- Define particle components. -->
	<component id="Rutile">
		<description>Rutile</description>
		<density>4.26</density>
		<molwt>79.87</molwt>
	</component>

	<!-- Define particle types. -->
	<particle id="[Ru]" model="bintree" subtree="false" track="primaryCoordinates">
	<!-- particle id="[Ru]" model="bintree" subtree="false" trackPrimarySeparation="true" -->
		<description>A rutile particle.</description>
		<fractdim>1.8</fractdim>
		<coalthresh>1.0</coalthresh>
		<sintering enable="true" model="GBD">
			<!-- GBD model params based on Eggersdorfer et al. (2012)-->
			<A units="s/m4K">9.112e+17</A>
			<E units="K">31030</E>
			<Dpmin units="m">0</Dpmin>
		</sintering>
	</particle>

	<!-- Coagulation -->
	<coagulation>
		<A>1.0</A>
		<kernel>transition</kernel>
	</coagulation>
	
</mechanism>