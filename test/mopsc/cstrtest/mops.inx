<mops version="2">
  
  <!-- Default parameters (can be overidden). --> 
  <runs>1</runs>
  <iter>1</iter>
  <atol>1.0e-9</atol> 
  <rtol>1.0e-3</rtol>
  <pcount>4096</pcount>
  <!-- maxm0>1e14</maxm0 -->
  <maxm0>1e14</maxm0>
  <relax>0.5</relax>
  

  <!-- Reactor definition (given initial conditions). -->
  <reactor type="cstr" constt="false" constv="false" id="Test_System" units="mol/mol">
    <component id="TICL4">0.5195</component>
    <component id="AR">0.052</component>
    <component id="O2">0.4211</component>
    <component id="CL2">0.0074</component>

        <inflow id="TiCl4">0.5195</inflow>
        <inflow id="Ar">0.0984</inflow>
        <inflow id="O2">0.4211</inflow>

    <residencetime unit="s">0.02</residencetime>    

    <temperature units="K">1500</temperature>
    <pressure units="bar">2.00</pressure>
  
  </reactor>

  <!-- Output time sequence. -->
  <timeintervals splits="1">
    <start>0.0</start>
    <time steps="100" splits="100">0.010</time>
    <time steps="100" splits="100">0.020</time>
    <time steps="100" splits="100">0.030</time>
    <time steps="100" splits="100">0.040</time>
    <time steps="100" splits="100">0.050</time>
  </timeintervals>
  
  <!-- Simulation output settings. -->
  <output>
    <filename>J3</filename>
    <statsbound property="dcol">
      <lower>0</lower>
      <upper>1.0e30</upper>
    </statsbound>
    
    <console interval="1" msgs="true">
      <tabular>
        <column fmt="sci">time</column>
        <!--column fmt="sci">A4</column-->
        <column fmt="float">#sp</column>
        <column fmt="sci">TiCl4</column>
        <column fmt="sci">Ar</column>
        <column fmt="sci">O2</column>
        <!--column fmt="sci">fv</column-->
        <column fmt="sci">ct</column>
      </tabular>
    </console>
    <ptrack enable="false" ptcount="50"/>
    <fluxanalysis enable="false">
        <element id="Ar"/>
        <element id="Ti"/>
        <element id="Cl"/>
        <element id="O"/>
    </fluxanalysis>

    
    <!-- File name for output (excluding extensions). -->
  </output>
</mops>
