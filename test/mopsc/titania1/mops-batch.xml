<?xml version="1.0" encoding="ISO-8859-1"?>
<mops version="2">
  <!-- Default parameters (can be overidden) --> 
  <runs>1</runs>
  <iter>20</iter>
  <atol>1.0e-17</atol> 
  <rtol>1.0e-4</rtol>
  <pcount>2048</pcount>
  <maxm0>4e12</maxm0> 
  <relax>0.8</relax>
  
    <reactor type="batch" constt="true" constv="true" id="Test_System" units="mol/mol">
        <temperature units="K">1200</temperature>
        <pressure units="bar">4.00</pressure>	
        <component id="TICL4">0.4681</component>
        <component id="O2">0.5319</component>
   </reactor>

  <!-- Output time sequence -->
  <timeintervals>
    <start>0.0</start>
	<time steps="100" splits="10">0.001</time>
  </timeintervals>

  <!-- Simulation output settings -->
  <output>
    <filename>PP</filename>
    <statsbound property="dcol">
      <lower>0</lower>
      <upper>1.0e30</upper>
    </statsbound>
    <console interval="1" msgs="true">
      <tabular>
        <column fmt="sci">time</column>
        <column fmt="float">#sp</column>
        <column fmt="sci">TICL4</column>
        <column fmt="sci">O2</column>
        <column fmt="sci">M0</column>
        <column fmt="sci">T</column>
      </tabular>
    </console>
    <ptrack enable="true" ptcount="1"/>
  </output>
</mops>
