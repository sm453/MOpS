<mechanism name="Titanium Dioxide" units="CGS">
           
    <!-- Define particle components -->
    <component id="Rutile">
        <description>Rutile</description>
        <density>4.26</density>
        <molwt>79.87</molwt>
    </component>
	
    <!-- Define particle types (Detailed) -->
