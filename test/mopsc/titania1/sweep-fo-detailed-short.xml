	<!-- Define particle types (Spherical) -->
    <particle id="[Ru]" model="spherical" subtree="false" hybrid="true">
        <description>A rutile particle.</description>
    </particle>
	
    <!-- Coagulation -->
    <coagulation>
	    <A>0.01</A>
	    <kernel>weightedconstant</kernel>
		<weightrule>w3</weightrule>
    </coagulation>

    <!-- Ti addition reactions
         A is in cm/s * Avogadro's number
         E is in kcal/mol -->    
    <reaction type="titania" name="TiCl4 addition" form="firstorder" defer="true">
        <formula>[Rutile_n] + TICL4 + 1.0 O2 to Rutile_n+1 + 2.0 CL2</formula>
        <reactant id="TICL4" stoich="+1.0" />
        <reactant id="O2"  stoich="+1.0" />
        <product id="CL2" stoich="+2.0" />
        <particlein  id="[Ru]"/>
        <particleout id="[Ru]"/>
        <!--A>1.2044e+028</A-->
		<A>0.0</A>
        <!--E units="cal">11.9503</E--> 
        <n>0.0</n>
        <!--A>8.06948e+028</A-->
        <E units="cal">14.3404</E> 
        <particleterm id="s" power="1.0"/>
        <component id="Rutile" dx="+1.0"/>
    </reaction>  

    <!-- [Ru] particle Inceptions
        m, d are in g, cm -->       
    
