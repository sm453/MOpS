<?xml version="1.0" encoding="utf-8"?>
<mops version="3">
    <!-- Sim parameters -->
    <runs>1</runs>
    <iter>10</iter>
    <atol>1.0e-17</atol>
    <rtol>1.0e-4</rtol>
    <maxm0>5e19</maxm0>
    <pcount>4096</pcount>
    <relax>0.8</relax>
	
	<!-- Initial reactor composition -->
    <mixture id="reac_mixture">
        <temperature units="K">573</temperature>
        <pressure units="bar">4.5</pressure>	
        <component id="TICL4">0.26</component>
        <component id="O2">0.74</component>
    </mixture>
	
	<!-- Initial reactor composition -->
    <mixture id="init_mixture">
        <temperature units="K">1500</temperature>
        <pressure units="bar">4.5</pressure>
	    <!--component id="AR">1.0</component-->
        <component id="TICL4">0.0001</component>
        <component id="O2">0.595</component>
        <component id="CL">0.0168</component>
        <component id="CL2">0.388</component>
        <component id="CLO">0.0001</component>	
		<population>
		    <m0>2.9311e+17</m0>
			<file>Network(stage1)(0)-SP(10000).ens</file>
		</population>
    </mixture>
	
	<!-- Initial reactor composition -->
    <mixture id="o2_mixture">
        <temperature units="K">3000</temperature>
        <pressure units="bar">4.5</pressure>
	    <component id="O2">1.0</component>
    </mixture>
	
    <!-- Stage 1 -->
    <reactor constt="false" constv="true" id="stage2" type="psr" units="mol/mol" order="1" dtype="cdelete">
	    <initial>init_mixture</initial>
        <flow dir="in" frac="0.62">o2_mixture</flow>
        <flow dir="in" frac="0.38">reac_mixture</flow>
        <restime unit="s">0.002</restime>
    </reactor>
	
    <!-- Time stepping -->
    <timeintervals>
      	<start>0.0</start>
        <time splits="50" steps="10000">0.01</time>
    </timeintervals>
   
    <!-- Output specifications -->
    <output>
        <statsbound property="dcol">
            <lower>0</lower>
            <upper>1.0e30</upper>
        </statsbound>
        <console interval="1" msgs="true">
            <tabular>
                <column fmt="sci">time</column>
	            <column fmt="sci">CT</column>
                <column fmt="float">#sp</column>
                <column fmt="sci">TICL4</column>
                <column fmt="sci">O2</column>
                <column fmt="sci">M0</column>
            </tabular>
        </console>
        <ptrack enable="true" ptcount="1"/>
        <filename>Network</filename>
    </output>
</mops>
