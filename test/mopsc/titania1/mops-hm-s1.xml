<?xml version="1.0" encoding="utf-8"?>
<mops version="3">
    <!-- Sim parameters -->
    <runs>3</runs>
    <iter>10</iter>
    <atol>1.0e-17</atol>
    <rtol>1.0e-4</rtol>
    <maxm0>0.512e14</maxm0>
    <pcount>1024</pcount>
    <relax>0.8</relax>
	
	<!-- Initial reactor composition -->
    <mixture id="initial">
        <component id="AR">1.0</component>
        <temperature units="K">1500</temperature>
        <pressure units="bar">3</pressure>
    </mixture>
	
    <!-- TiCl4 injected to stages 1 and 2 -->
    <mixture id="TiCl4_injection">
        <component id="TICL4">0.26</component>
        <component id="O2">0.74</component>
        <temperature units="K">600</temperature>
        <pressure units="bar">3</pressure>
    </mixture>
	
    <!-- O2 plasma feed to stage 1 -->
    <mixture id="O2_plasma">
        <component id="O2">1.0</component>
        <!--component id="O">0.1</component-->
        <temperature units="K">3000</temperature>
	    <pressure units="bar">3</pressure>
    </mixture>
	
    <!-- Stage 0 -->
    <!--reactor constt="false" constv="true" id="stage0" type="psr" units="mol/mol" order="1" dtype="cdelete">
	    <initial>O2_plasma</initial>
        <flow dir="in" frac="1.0">O2_plasma</flow>
        <restime unit="s">0.003</restime>
    </reactor-->
	
    <!-- Stage 1 -->
    <reactor constt="false" constv="true" id="stage1" type="psr" units="mol/mol" order="1"> <!-- dtype="cdelete" -->
	    <initial>initial</initial>
	    <!--doubling enable="false"></doubling-->
        <flow dir="in" frac="0.24">O2_plasma</flow>
        <flow dir="in" frac="0.76">TiCl4_injection</flow>
        <restime unit="s">0.003</restime>
    </reactor>
	
	<!-- Time stepping -->
    <timeintervals>
      	<start>0.0</start>
        <time splits="5" steps="9000">0.003</time>
        <!--time splits="2" steps="25000">0.1</time-->
    </timeintervals>
   
    <!-- Output specifications -->
    <output>
        <statsbound property="dcol">
            <lower>0</lower>
            <upper>1.0e30</upper>
        </statsbound>
        <console interval="1" msgs="true">
            <tabular>
                <column fmt="sci">time</column>
	            <column fmt="sci">CT</column>
                <column fmt="float">#sp</column>
                <column fmt="sci">TICL4</column>
                <column fmt="sci">O2</column>
                <column fmt="sci">M0</column>
            </tabular>
        </console>
        <ptrack enable="true" ptcount="1"/>
        <filename>Network</filename>
    </output>
</mops>
