<?xml version="1.0" encoding="ISO-8859-1"?>
<brush version="2">
  
  <!-- Default parameters (can be overidden). --> 
  <runs>2</runs>
  <iter>2</iter>
  <atol>1.0e-2</atol> 
  <rtol>1.0e-4</rtol>
  <pcount>1024</pcount>
  <maxm0>2.0e17</maxm0>
  <relax>0.5</relax>
  


  <!-- Output time sequence. -->
  <timeintervals splits="1">
    <start>0.0</start>
    <time steps="1" splits="1">0.01</time>
    <time steps="1" splits="1">0.02</time>
    <time steps="1" splits="1">0.1</time>
    <time steps="1" splits="1">0.2</time>
  </timeintervals>


  <output>
    <filename>TestOut</filename>
  </output>
  
</brush>
